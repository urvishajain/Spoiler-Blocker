{
    "manifest_version": 3,
    "name": "Endgame Spoiler Blocker",
    "version": "0.1",
    "description": "Removes spoiler words of endgame",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    }
     "action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "19": "images/icon19.png",
        "38": "images/icon38.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"

    }
  }

